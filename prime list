#include<iostream>
using namespace std;

bool checkPrime(int n){
  int i = 2;
  for(int i = 2; i<n;i++){
    if(n%i==0){
      return false;
    }
  }
  return true;
}

int main()
{ int n;
 cout<<"Enter n to get prime numbers upto n: "<<endl;
 cin>>n;
 for(int i=1;i<=n;i++){
   bool isiprime = checkPrime(i);
   if(isiprime){
     cout<<i<<" ";

   }
 }

  //cout<<"HELLOOOÃ’";
}
